{
    "analyzers": {
      "core": {
        "enabled": true,
        "rules": {
          "no-unused-params": {
            "level": "warning"
          },
          "outputs-should-not-contain-secrets": {
            "level": "warning"
          },
          "no-hardcoded-location": {
            "level": "warning"
          },
          "no-hardcoded-env-urls": {
            "level": "warning"
          }
        }
      }
    }
  }
  